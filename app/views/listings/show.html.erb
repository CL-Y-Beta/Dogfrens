<div class="container">
  <h1><%= @listing.title %></h1>
  <%= render @listing %>

  <div>
    <%= link_to "Edit this listing", edit_listing_path(@listing) %>
    <%= link_to "Back to listings", root_path %>
    <%# <%= link_to "Book this", %>
    <%= link_to "Bookmark this", new_bookmark_path %>
    <%= button_to "Destroy this listing", @listing, method: :delete, data: { confirm: "Are you sure?" } %>
  </div>
</div>
